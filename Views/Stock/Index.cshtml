@model List<Stock>

<h2>Inventory</h2>

<table class="table">
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var stock in Model)
        {
            <tr>
                <td>@stock.Product.ProductName</td>
                <td>@stock.Quantity</td>
            </tr>
        }
    </tbody>
</table>
